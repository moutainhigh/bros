					<xmlField tagName="currencyUomId"  dataName="currencyUomId"/><!--货币-->
				 </xmlTag>
				 <xmlField tagName="statusName" dataName="statusName"/><!--状态 -->
	             <xmlField tagName="orderDate" dataName="orderDate"/><!--订单日期-->
	             <xmlField tagName="partyId" dataName="partyId"/><!--客户标识-->
	          </xmlTag>
         </xmlList>
           <xmlField tagName="orderListSize" dataName="orderListSize"/><!--总订单数 -->
