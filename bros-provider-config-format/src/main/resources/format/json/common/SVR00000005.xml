	<fmtDef id="SVR00000005RecFmt">
	      <xmlRel rel="RecFmt"/>
          <xmlList tagName="orderList" dataName="orderList"><!--订单列表 -->
          	  <xmlTag>
	             <xmlField tagName="orderTypeName" dataName="orderTypeName"/><!--订单类型 -->
	             <xmlField tagName="orderId" dataName="orderId"/><!--订单标识-->
	             <xmlField tagName="fullName" dataName="fullName"/><!--名字-->
	             <xmlField tagName="totalOrderItemsQuantity" dataName="totalOrderItemsQuantity"/><!--订单项数量 -->
	             <xmlField tagName="orderBackorderQuantity" dataName="orderBackorderQuantity"/><!--订单缺货数量 -->
	             <xmlField tagName="orderReturnedQuantity" dataName="orderReturnedQuantity"/><!--退货项数量 -->
