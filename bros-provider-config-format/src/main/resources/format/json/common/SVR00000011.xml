<?xml version="1.0" encoding="UTF-8" ?>
<formats>
	<fmtDef id="SVR00000011SendFmt">
		     <xmlRel rel="SendFmt"/>
			 <xmlField tagName="partyId" dataName="partyId"/><!--当事人标识  -->
	</fmtDef>
