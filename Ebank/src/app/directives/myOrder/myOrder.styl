.financial-trade-item
    color:#333
    font-size 12px
    .financial-trade
        padding:0px 12px 0px 12px
    table
        width 100%
        border-bottom solid 1px #e8e8e8
    thead
        height 60px
        line-height 60px
        border-bottom:1px solid #ccc

    td
